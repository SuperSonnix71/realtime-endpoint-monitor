        "dotenv": "^17.2.3",
        "fastify": "^5.7.4",
        "fastify-sse-v2": "^4.0.2",
        "pg": "^8.18.0",
        "pino": "^9.0.0",
        "prisma": "^5.15.0",
        "undici": "^6.16.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20.12.7",
        "@types/pg": "^8.16.0",
        "eslint": "^9.39.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "vitest": "^1.4.0"
    }
