{
    "name": "@monitor/backend",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "ts-node-esm src/server/index.ts",
        "build": "tsc -p tsconfig.json",
